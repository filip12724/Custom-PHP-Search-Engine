    <script>
        (function() {
            const btnFlowItems = document.querySelectorAll('.btn-flow__item');
            const btnFlowButton = document.querySelector('.btn-flow__btn');
            const btnFlowCurrentLink = btnFlowButton.getAttribute('href');

            const clearCheckboxes = () => {
                btnFlowItems.forEach((item) => {
                    item.classList.remove('active');
                });
            };

            document.querySelector('.btn-flow__list').addEventListener('click', (e) => {
                const item = e.target.closest('.btn-flow__item');
                if (!item)
                    return;

                e.preventDefault();

                if (item.classList.contains('active')) {
                    clearCheckboxes();
                    btnFlowButton.setAttribute('href', btnFlowCurrentLink);
                    btnFlowButton.classList.remove('btn-active');
                } else {
                    clearCheckboxes();
                    item.classList.add('active');
                    // Get the value of the selected checkbox
                    const selectedValue = item.querySelector('.btn-flow__checkbox').value;
                    btnFlowButton.setAttribute('href', selectedValue);
                    btnFlowButton.classList.add('btn-active');
                }

                // Add bounce animation to the button for 700ms
                btnFlowButton.classList.add('btn-flow__btn--bounce');
                setTimeout(() => {
                    btnFlowButton.classList.remove('btn-flow__btn--bounce');
                }, 700);
            });
        })();
    </script>

