  window._hsp.push = function(e) {
    if (e[0] === 'setBannerSettings') {
        // Replace with our own
        var settings = {};
        settings[window.location.hostname] = [{
          id: '',
          portalId: '4768971',
          domain: '',
          label: '',
          enabled: true,
          configuration: {
              path: '/'
          },
          policy: {
            type: 1, // NO_COOKIES
            text: ''
          },
          legacyCustomization: {
          }
        }];
        o_hsp_push(['setBannerSettings', settings]);
    } else {
      o_hsp_push(e);
    }
  };
  window._hsp.push(['addPrivacyConsentListener', a => {
    //console.log('HubSpot', a)
  }]);
  // IAB https://support.didomi.io/iab-tcf-v2-new-purposes/features-summary
  // Simplified flags for Sonobi necessary consents
  function sbiIsPurposeConsentSonobi(consents) {
    return consents
      && consents[1] /* Purpose 1 - Store and/or access information on a device */
      && consents[2] /* Purpose 2 - Select basic ads */
      && consents[3] /* Purpose 3 - Create a personalized ads profile */
      && consents[4]; /* Purpose 4 - Select personalized ads */
  }
  function sbiIsVendorConsentSonobi(consents) {
    return consents
      && consents[104]; /* Sonobi vendor */
  }
  // Cookie delete
  function delC(names) {
    names.forEach(function(name) {
      document.cookie = name + "=; expires=Thu, 01 Jan 1970 00:00:00 GMT; path=/; domain=sonobi.com";
    });
  }
  // Deny
  function deny() {
    // HubSpot - delay because HubSpot will setConsent(true) at page load
    window._hsp = window._hsp || [];
    window._hsq = window._hsq || [];
    window._hsp.push(['revokeCookieConsent']);
    window._hsq.push(['doNotTrack']);
    delC(['__hssc', '__hssrc', '__hstc', 'hubspotutk', 'messagesUtk', '__hs_initial_opt_in', '_fbp']);
  }
  // Grant
  function grant() {
    // HubSpot
    window._hsq = window._hsq || [];
    window._hsq.push(['doNotTrack', { track: true }]);
  }
  if (window.__tcfapi) {
    window.__tcfapi('addEventListener', 2, function(tcData, listenerSuccess) {
      if (listenerSuccess) {
        // Check the eventStatus -- These are if the consent is already set or just updated
        if (tcData.eventStatus === 'useractioncomplete' || tcData.eventStatus === 'tcloaded') {
          if ((!tcData.purpose || sbiIsPurposeConsentSonobi(tcData.purpose.consents)) && (!tcData.vendor || sbiIsVendorConsentSonobi(tcData.vendor.consents))) {
            grant();
          } else {
            deny();
          }
        } else if (!tcData.tcString || tcData.tcString.length === 0) {
          // Default with no consent while user is deciding
          deny();
        }
      }
    });
  }
</script>
