<script nonce="quhml2ka83v0hLUOCob3G6OwaxK1q/MDNHhZfpWV1QM=">
    //Trusted Types support check. If not supported use Tinyfill.
    const trustedTypesLibrary =
        typeof window.trustedTypes !== 'undefined' &&
            typeof window.trustedTypes.createPolicy !== 'undefined'
            ? window.trustedTypes
            : { createPolicy: (_name, rules) => rules };

    //Trusted Types policy validating loading files from cdn
    const cdnScriptsPolicy = trustedTypesLibrary.createPolicy('cdn-url#srv', {
        createScriptURL(url) {
            //obtain CDN base path from config
            if (url.startsWith(configuration.cdnBasePath)) {
                return url;
            }
            console.log('Invalid CDN URL' + url);
        },
    });

    function loadFile(path, type) {
        if (type == "js") {
            var fileref = document.createElement("script")
            fileref.setAttribute("type", "text/javascript")
            //Trusted Types check for this js src sink function.
            fileref.setAttribute("src", cdnScriptsPolicy.createScriptURL(path))
        } else if (type == "css") {
            var fileref = document.createElement("link")
            fileref.setAttribute("rel", "stylesheet")
            fileref.setAttribute("type", "text/css")
            fileref.setAttribute("href", path)
        }
        document.getElementsByTagName("head")[0].appendChild(fileref)
    }
    function loadScripts(data) {
        var basePath = configuration.cdnBasePath;
        data["unauth"].css.forEach(function(path) {
            loadFile(basePath + path.toLowerCase(), "css")
        });

        data["unauth"].js.forEach(function(path) {
            loadFile(basePath + path.toLowerCase(), "js")
        });
    }

    function loadAssets() {
        if (window.fetch !== undefined) {
            fetch(configuration.ocvCdnPath + '/build/webpack.assets.json', {
                method: 'get'
            }).then(function(response) {
                return response.json();
            }).then(loadScripts).catch(function(err) {
            });
        }
        else {
            // IE11 scenario
            let xhr = new XMLHttpRequest();

            xhr.overrideMimeType("application/json");
            xhr.open('GET', configuration.ocvCdnPath + '/build/webpack.assets.json');
            xhr.onreadystatechange = function() {
                if (xhr.readyState !== 4) return;

                if (xhr.status === 200) {
                    loadScripts(JSON.parse(xhr.responseText));
                }
            };

            xhr.send();
        }
    }

    loadAssets();
</script>
</html>";}