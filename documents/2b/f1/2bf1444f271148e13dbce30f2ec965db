				<div class="elementor-widget-container">
					<script>

    current_slide = 1;

    slideInterval = null;

    function slider_advance(sDirection) {
        slide_count = jQuery('.main-slide').length;

        if (sDirection == 'p') {
            current_slide -= 1;
        }
        else {
            current_slide += 1;
        }

        if (current_slide == 0) {
            current_slide = slide_count;
        }
        else if (current_slide > slide_count) {
            current_slide = 1;
        }

        //console.log('SWITCH TO SLIDE: ' + current_slide);
        jQuery('.main-slide').css('display','none');
        jQuery('#main-slide-' + current_slide).css("display","block");

        //console.log('SLIDE ADVANCED');

        if (sDirection != 'auto' && slideInterval != null) {
            //console.log('CLEAR INTERVAL');
            clearInterval(slideInterval);
            start_autoplay();
            
        }
    }

    function start_autoplay() {
        slideInterval = setInterval(slider_advance, 7000, 'auto');
        //console.log('START INTERVAL');
    }

    start_autoplay();

</script>
<style>
   .slider-buttons {
      display: flex;
      align-items:  center
}
 .slider-buttons button:hover {
     color: #ffffff;
     transition: all .5s;
 }
   .slider-buttons button {
       background: transparent;
       border: 0px;
       padding: 16px 21px;
       font-size: 25px;
       transition: all .5s;
       color: #ffffff99;
       background: linear-gradient(125deg, #ffffff38, #ffffff16)
}
#slider_prev_button {
margin-right: 0;
}
#slider_next_button {
margin-left: 13px;

}
@media only screen and (max-width: 767px) {
       .slider-buttons {
     justify-content: center !important;
}
.slider-buttons button {
       padding: 14px 18px;
       font-size: 20px;
}
}

.slider-buttons .slider_button {
    cursor: pointer;
}
</style>
<div class="slider-buttons">
    <button onClick = "slider_advance('p')" id="slider_prev_button" class="slider_button"><i class="fa-light fa-angle-left"></i></button>
<button onClick = "slider_advance('n')" id = "slider_next_button" class="slider_button"><i class="fa-light fa-angle-right"></i></button>
</div>				</div>
