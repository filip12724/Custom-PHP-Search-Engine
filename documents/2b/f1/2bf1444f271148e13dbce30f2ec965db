<script>
    handleJukeboxDispatchedEventsInPagePromoter = event => {
        switch (event.detail.eventName) {
            case "load_show":
                if (event.detail.promoter="in_page"){
                    const currentQS = new URLSearchParams(window.location.search);
                    if (currentQS.size>0){
                        document.querySelectorAll('#'+event.detail.promoterConfig.elementId+' a[href*=lb-mode]').forEach(function(h){
                        const linkHref = h.href
                        const linkHrefURL = new URL(linkHref)
                        currentQS.forEach(function(k,v){linkHrefURL.searchParams.append(k,v)})
                        h.href=linkHrefURL
                        })
                    }
                }
            break;
        }
    };

    window.addEventListener("pf_event", this.handleJukeboxDispatchedEventsInPagePromoter, false);

    const currentQS = new URLSearchParams(window.location.search);
    if (currentQS.size>0){
    document.querySelectorAll('div:not(#qa-website-inject) a[href*=lb-mode]').forEach(function(h){
        const linkHref = h.href
        const linkHrefURL = new URL(linkHref)
        currentQS.forEach(function(k,v){linkHrefURL.searchParams.append(k,v)})
        h.href=linkHrefURL

    })
    }
