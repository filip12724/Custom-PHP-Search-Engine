    <script>MktoForms2.loadForm("//cybersecurity.arcticwolf.com", "840-OSQ-661", 1002, function(form) {
        			
         var formElement = form.getFormElem()[0];
         formElement.addEventListener("submit", function(event) {
         window.SixSenseEvent = event;
         console.info("6sense: Form submit initiated.");
     });
     formElement.getElementsByTagName('button')[0].setAttribute('name', 'mktoButton_' + form.getId());
     formElement.setAttribute('name', 'mktoForm_' + form.getId());
     form.onSuccess(function() {
	try {	 
	 if (_6si) {
     _6si.send(window.SixSenseEvent);
        console.info("6sense: Form submit completed.");
     } else {
         console.warn("6sense: Event object not found. Form fill not logged.");
     }
	} catch (error) {console.error(error);}
     });
    });</script>

    <!-- Form Script for Styling Attributes-->
    <script type="text/javascript" src="https://cybersecurity.arcticwolf.com/rs/840-OSQ-661/images/AW-mkto-form-style-attributes-210628.js"></script>

    <!-- Floating label script-->
    <script>
    // requires mktoForm-floatingLabels.css (.activeLabel styles)
    // requires Marketo form label setting set to "Above" (instead of "Left")

        function floatingLabels() {
            jQuery('.mktoRadioList, .mktoCheckboxList').parent().children('label').addClass('d-none');
            jQuery('.mktoForm select:not("#Consent_Opt_In__c") option:nth-of-type(1)').html("");

            var inputs = jQuery('.mktoForm input[type=url], .mktoForm input[type=text], .mktoForm input[type=date], .mktoForm input[type=tel], .mktoForm input[type=email], .mktoForm input[type=number], .mktoForm textarea.mktoField, .mktoForm select.mktoField');

            jQuery(inputs).focus(function(){
                var fieldID = jQuery(this).attr('id');
                fieldLabel = 'label.mktoLabel[for=' + fieldID +']';
                jQuery(fieldLabel).addClass('activeLabel')
            });

            jQuery(inputs).blur(function(){
                var fieldID = jQuery(this).attr('id');
                fieldLabel = 'label.mktoLabel[for=' + fieldID +']';
                if (jQuery(this).val() == ""){
                    jQuery(fieldLabel).removeClass('activeLabel');
                }
            });
        }

        jQuery(window).on('load',function(){
            jQuery(document).ready(function() {
        	   MktoForms2.whenReady(function (form){
                 jQuery(floatingLabels)
                 jQuery('form.mktoForm').on('change', floatingLabels);
        	   });
            });
        });
    </script>

