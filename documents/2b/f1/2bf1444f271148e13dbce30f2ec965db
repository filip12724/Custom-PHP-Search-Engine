<script>
// When the user scrolls down 2px from the top of the document, resize the navbar's padding and the logo's font size
window.onscroll = function() {scrollFunction()};
 
function scrollFunction() {
                if (window.innerWidth > 1024) {
                                  if (document.body.scrollTop > 2 || document.documentElement.scrollTop > 2) {
                                document.getElementById("header-widget-area").style.transform = "translate(0, -87px)";
                                document.getElementById("header-widget-area").style.background = "black";
                                document.getElementById("header-logo-aw").style.transform = "translate(0, 57px)";
                                  } else {
                                  document.getElementById("header-widget-area").style.transform = "translate(0, 0)";
                                  document.getElementById("header-widget-area").style.background = "transparent";
								document.getElementById("header-logo-aw").style.transform = "translate(0, 0)";
                                  }
                }
}
	window.onresize = function() {resizeFunction()};
function resizeFunction() {
                if (window.innerWidth <= 1024) {
                    document.getElementById("header-widget-area").style.transform = "translate(0, 0)";
                    document.getElementById("header-widget-area").style.background = "transparent";
					document.getElementById("header-logo-aw").style.transform = "translate(0, 0)";
                }
}
</script>
