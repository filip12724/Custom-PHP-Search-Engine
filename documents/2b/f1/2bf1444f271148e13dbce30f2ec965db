<script>
document.addEventListener("DOMContentLoaded", function() {
  const skyElement = document.querySelector('.animated-sky');

  const observer = new IntersectionObserver((entries) => {
    entries.forEach(entry => {
      if (entry.isIntersecting) {
        skyElement.classList.add('fade-in-title-visible');
      } else {
        skyElement.classList.remove('fade-in-title-visible');
      }
    });
  }, { threshold: 0.025 });

  observer.observe(skyElement);
});
</script>
