<script>
        // Function to add the active class when input is focused
        function search_active() {
            document.getElementById('search-input').classList.add('active');
        }

        // Function to remove the active class when clicking outside the input
        document.addEventListener('click', function(event) {
            const inputField = document.getElementById('search-input');
            if (!inputField.contains(event.target)) {
                inputField.classList.remove('active');
            }
        });
    </script>


