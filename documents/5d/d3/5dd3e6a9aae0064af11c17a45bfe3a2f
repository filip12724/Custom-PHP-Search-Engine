#stickyheaders{
transition : transform 0.34s ease;
}
.headerup{
transform: translateY(-70px); /*adjust this value to the height of your header*/
}
</style>

<script>
(function () {
    // Function to scroll to the target section using direct positioning
    function scrollToAnchor(anchor) {
        const target = document.querySelector(anchor);
        if (target) {
            setTimeout(() => {
                const offsetTop = target.getBoundingClientRect().top + window.scrollY;
                window.scrollTo({
                    top: offsetTop,
                    behavior: 'smooth',
                });
            }, 500); // Small delay to override any other script
        }
    }

    // Extract 'anchor' parameter from the URL
    function getAnchorFromQuery() {
        const params = new URLSearchParams(window.location.search);
        const anchorParam = params.get('anchor');
        if (anchorParam) {
            return `#${anchorParam}`;
        }
        return null;
    }

    // Wait until the page is fully loaded and all scripts have run
    window.addEventListener('load', function () {
        const anchor = getAnchorFromQuery();
        if (anchor) {
            scrollToAnchor(anchor);
        }
    });
})();



</script>
