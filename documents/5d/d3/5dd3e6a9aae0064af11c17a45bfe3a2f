<script>
document.addEventListener('DOMContentLoaded', function() {
    const slider = document.querySelector('.taxonomy-filter .elementor-widget-container');
    let isDown = false;
    let startX;
    let scrollLeft;

    if (slider) {
        slider.addEventListener('mousedown', (e) => {
            isDown = true;
            slider.classList.add('active');
            startX = e.pageX - slider.offsetLeft;
            scrollLeft = slider.scrollLeft;
        });

        slider.addEventListener('mouseleave', () => {
            isDown = false;
            slider.classList.remove('active');
        });

        slider.addEventListener('mouseup', () => {
            isDown = false;
            slider.classList.remove('active');
        });

        slider.addEventListener('mousemove', (e) => {
            if (!isDown) return; // Stop if not holding down the mouse
            e.preventDefault();
            const x = e.pageX - slider.offsetLeft;
            const walk = (x - startX) * 3; // Adjust scroll speed if needed
            slider.scrollLeft = scrollLeft - walk; // Inverted scroll
        });
    }
});
</script>
<script>
document.addEventListener("DOMContentLoaded", function () {
    // Wait for the HubSpot form to load
    setTimeout(function () {
        const formFields = document.querySelectorAll('.hs-form .hs-form-field');

        formFields.forEach(field => {
            const label = field.querySelector('label');
            const input = field.querySelector('input:not([type="checkbox"]), textarea');
            const select = field.querySelector('select');

            if (label) {
                const labelText = label.innerText.trim();

                // Handle input and textarea fields
                if (input) {
                    input.setAttribute('placeholder', labelText); // Set label as placeholder
                    label.style.display = 'none'; // Hide the label
                }

                // Handle select fields
                if (select) {
                    let placeholderOption = select.querySelector('option[value=""]');

                    if (placeholderOption) {
                        // If a placeholder option exists, update its text
                        placeholderOption.textContent = labelText;
                    } else {
                        // If no placeholder option exists, create one
                        placeholderOption = document.createElement('option');
                        placeholderOption.textContent = labelText;
                        placeholderOption.value = "";
                        placeholderOption.disabled = true;
                        placeholderOption.selected = true;

                        // Prepend the new placeholder option
                        select.prepend(placeholderOption);
                    }

                    label.style.display = 'none'; // Hide the label
                }
            }
        });
    }, 300); // Adjust delay as necessary for HubSpot form to load
});

</script>

<script>



document.addEventListener('DOMContentLoaded', function() {
jQuery(function($) {
var mywindow = $(window);
var mypos = mywindow.scrollTop();
let scrolling = false; /* For throlling scroll event */
window.addEventListener('scroll', function() {
scrolling = true;
});
setInterval(() => {
if (scrolling) {
scrolling = false;
if (mypos > 40) {
if (mywindow.scrollTop() > mypos) {
$('#stickyheaders').addClass('headerup');
} else {
$('#stickyheaders').removeClass('headerup');
}
}
mypos = mywindow.scrollTop();
}
}, 300);
});
});

</script>
