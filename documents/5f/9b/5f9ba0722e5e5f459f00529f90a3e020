<script>
  document.addEventListener('DOMContentLoaded', function() {
    var elements = document.querySelectorAll('.c-marketecture-diagram__desktop .c-marketecture-diagram__trigger');
    var targetDiv = document.querySelector('.c-marketecture-diagram');

    elements.forEach(function(element) {
      element.addEventListener('mouseenter', function() {
        var productValue = this.getAttribute('data-product');
        targetDiv.setAttribute('data-hovered', 'true');
        targetDiv.setAttribute('data-active', productValue);
      });

      element.addEventListener('mouseleave', function() {
        targetDiv.removeAttribute('data-active');
        targetDiv.removeAttribute('data-hovered', 'true');
      });
    });
  });
</script>

