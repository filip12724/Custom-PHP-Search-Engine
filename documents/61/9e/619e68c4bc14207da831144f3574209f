            <script type="text/javascript">
            document.addEventListener('DOMContentLoaded', function () {
                var lazyVideos = [].slice.call(document.querySelectorAll('video.lazy'));

                if ('IntersectionObserver' in window) {
                    var lazyVideoObserver = new IntersectionObserver(function (entries, observer) {
                        entries.forEach(function (video) {
                            if (video.isIntersecting) {
                                for (var source in video.target.children) {
                                    var videoSource = video.target.children[source];

                                    if (typeof videoSource.tagName === 'string' && videoSource.tagName === 'SOURCE') {
                                        videoSource.src = videoSource.dataset.src;
                                    }
                                }

                                video.target.load();
                                video.target.classList.remove('lazy');
                                lazyVideoObserver.unobserve(video.target);
                            }
                        });
                    });
                    lazyVideos.forEach(function (lazyVideo) {
                        lazyVideoObserver.observe(lazyVideo);
                    });
                }
            });
        </script>
                                    <script type="rocketlazyloadscript" data-rocket-type="text/javascript">
                                window.addEventListener('DOMContentLoaded', function () {
                                    function showPopupOnPageLoad() {
                                        var queryString = document.location.hash || document.location.search;
                                        let requestDemoPattern = /\bdemo\b/i;
                                        let getQuotePattern = /\bgetquote\b/i;
                                        if (queryString == '') {
                                            return;
                                        }

                                        queryString = queryString.toLowerCase();

                                        if (!requestDemoPattern.test(queryString) && !getQuotePattern.test(queryString)) {
                                            return;
                                        }

                                        var popupTimeout = window.setTimeout(function () {
                                            if (requestDemoPattern.test(queryString)) {
                                                if (jQuery('.popup-demo').length > 0) {
                                                    jQuery('.popup-demo:first').click();
                                                } else if (jQuery('.popup-threefields').length > 0) {
                                                    jQuery('.popup-threefields:first').click();
                                                } else if (jQuery('.popup-desktop-country-phone').length > 0) {
                                                    jQuery('.popup-desktop-country-phone:first').click();
                                                } else if (jQuery('.popup-threefields-workemail').length > 0) {
                                                    jQuery('.popup-threefields-workemail:first').click();
                                                }
                                            } else if (getQuotePattern.test(queryString)) {
                                                jQuery('.popup-goute:first').click();
                                            }

                                            clearTimeout(popupTimeout);
                                        }, 1000);
                                    }
                                    if((jQuery('html[lang=en-US]').length <= 0 && jQuery('html[lang=ja-JP]').length <= 0) || jQuery('body').hasClass('single-funnel_gated_content')) {
                                        showPopupOnPageLoad();
                                    }
                                    //footer chatbot issue on page zoom
                                    function removeChatBotZoom() {
                                        if (jQuery("body").css("zoom") !== "1") {
                                            jQuery('#insent-iframe').css('zoom', '1.25');
                                        } else {
                                            jQuery('#insent-iframe').css('zoom', '');
                                        }
                                    }
                                    window.addEventListener('resize', function (event) {
                                        removeChatBotZoom();
                                    }, true);
                                    var existCondition = setInterval(function () {
                                        if (jQuery('#insent-iframe').length > 0) {
                                            removeChatBotZoom();
                                            clearInterval(existCondition);
                                        }
                                    }, 100);
                                });
                            </script>
                                                        <script type="rocketlazyloadscript">
                                                                const current_lang = (document.querySelector('html').hasAttribute('lang') && (document.querySelector('html').getAttribute('lang') == 'ja-JP')) ? 'JP' : "";
                                const redirectionRuleForm = (occurence, type) => {                                   
                                    occurence.addEventListener('click', (e) => {
                                         console.info(e);
                                        if(document.querySelector('html').hasAttribute('lang') && (document.querySelector('html').getAttribute('lang')==='en-US' || document.querySelector('html').getAttribute('lang')==='ja-JP')){
                                            e.preventDefault();
                                            if (occurence.getAttribute('data-pardot-type') === null) {
                                                localStorage.removeItem("button_cta_value");
                                            } else {
                                                let pardotType = occurence.getAttribute('data-pardot-type');
                                                localStorage.setItem("button_cta_value", pardotType);
                                            }
                                            redirectToDemoLandingPageFooter(type);
                                        }
                                    });
                                }
                                const workflowDemo = 'workflow-demo'
                                document.querySelectorAll('a').forEach(occurence => {
                                    if (occurence.href.indexOf(window.location.origin + "/walkme-quote") > -1) {
                                        occurence.href =  getRedirectURL("/walkme-quote/");                                      
                                        redirectionRuleForm(occurence, 'quote');
                                    } else if (occurence.href.indexOf(window.location.origin + "/jp/walkme-demo") > -1) {
                                        occurence.href =  getRedirectURL("/jp/walkme-demo/");
                                        redirectionRuleForm(occurence, 'demo-jp');
                                    }else if (occurence.href.indexOf(window.location.origin + "/walkme-demo") > -1) {
                                        occurence.href =  getRedirectURL("/walkme-demo/");
                                        redirectionRuleForm(occurence, 'demo');
                                    } else if (occurence.href.indexOf(window.location.origin + "/"+workflowDemo) > -1) {
                                        occurence.href = getRedirectURL("/"+workflowDemo+"/");
                                        redirectionRuleForm(occurence, workflowDemo);
                                    }
                                    else if (occurence.href.indexOf(window.location.origin + "/ai/walkme-x-demo") > -1) {
                                        occurence.href =  getRedirectURL("/ai/walkme-x-demo/");
                                    }
                                });
                                document.querySelectorAll('body.japaneselang a.popup-demo').forEach(occurence => {
                                    if (occurence.href !=null && occurence.href.indexOf("javascript:void(0)") > -1) {
                                        occurence.href =  getRedirectURL("/jp/walkme-demo/");
                                    }
                                    redirectionRuleForm(occurence, 'demo-jp');
                                });

                                document.querySelectorAll('body.japaneselang button.popup-demo').forEach(occurence => {
                                    redirectionRuleForm(occurence, 'demo-jp');
                                });

                                
                                document.querySelectorAll('body:not(.japaneselang) a.popup-demo').forEach(occurence => {
                                    if (occurence.href !=null && occurence.href.indexOf("javascript:void(0)") > -1) {
                                        occurence.href =  getRedirectURL("/walkme-demo/");
                                    }
                                    redirectionRuleForm(occurence, 'demo');
                                });

                                document.querySelectorAll('body:not(.japaneselang) a.workflow-btn').forEach(occurence => {
