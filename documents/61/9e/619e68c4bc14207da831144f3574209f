            (function(w, d, s, l, i) {
                w[l] = w[l] || [];
                w[l].push({
                    'gtm.start': new Date().getTime(),
                    event: 'gtm.js'
                });
                var f = d.getElementsByTagName(s)[0],
                    j = d.createElement(s),
                    dl = l != 'dataLayer' ? '&l=' + l : '';
                j.async = true;
                j.setAttributeNode(d.createAttribute('data-ot-ignore'));
                j.src =
                    'https://www.googletagmanager.com/gtm.js?id=' + i + dl;
