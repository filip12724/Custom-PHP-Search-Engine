    <script>
      document.addEventListener("DOMContentLoaded", function() {
        const notificationCard = document.getElementById('notification-card');

        // Check if the notification has been closed in this session
        if (!sessionStorage.getItem('notificationClosed')) {
          setTimeout(() => {
            notificationCard.classList.add('show');
          }, 1000); // Delay to slide in
        }
      });

      function closeNotification() {
        const notificationCard = document.getElementById('notification-card');
        notificationCard.classList.remove('show');

        // Set session storage to keep it closed
        sessionStorage.setItem('notificationClosed', 'true');
      }
    </script>

