        window.attachEvent("onmessage", function(event) {
          window.__cmp.receiveMessage(event);
        }, false);
      } else {
        window.addEventListener("message", function(event) {
          window.__cmp.receiveMessage(event);
        }, false);
      }

      function addLocatorFrame() {
        if (!window.frames['__cmpLocator']) {
