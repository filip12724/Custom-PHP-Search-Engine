window['optimizely'] = window['optimizely'] || [];
window['get_ga_id']=function(){
  let myCookie = '';
  const value = '; ' + document.cookie;
  const parts = value.split('; _ga=');
  if (parts.length == 2)
      myCookie = parts.pop().split(';').shift();
  const regex=/^GA\d\.\d\./;
  if(regex.test(myCookie))
    myCookie = myCookie.replace(regex, '');
  return myCookie;
};</script>
